@page
@model GinasioIsla.Pages.Inscricoes.IndexModel

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-page="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Inscricao[0].Aluno)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Inscricao[0].Modalidade)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model.Inscricao) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Aluno.ID)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Modalidade.ModalidadeID)
            </td>
            <td>
                <a asp-page="./Edit" asp-route-id="@item.InscricaoID">Edit</a> |
                <a asp-page="./Details" asp-route-id="@item.InscricaoID">Details</a> |
                <a asp-page="./Delete" asp-route-id="@item.InscricaoID">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
